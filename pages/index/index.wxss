.top{
    width:100%;
    height:60rpx;
}
.top .city{
    width:20%;
    font-size: 30rpx;
    float: left;
}
.top .city view{
    line-height:46rpx;
    float:left;
}
.top .city image{
    width:40rpx;
    margin-top:6rpx;
}
.top .search .button{
    font-size:30rpx;
    float: left;
    background:#EEEEEE;
    color:#929292;
    width:72%;
    padding:6rpx 4%;
    border-radius:50rpx;
}

.userCar{
    height:150rpx;
    padding:40rpx 60rpx;
    margin-top:16rpx;
    border-radius:16rpx;
    box-shadow:0 0 8px #A6A6A6;
}
.userCar .top{
    font-size:34rpx;
}
.userCar .top .numOne{
    float:left;
}
.userCar .top .numTwo{
    width:62%;
    float:right;
}
.userCar .center{
    margin-top:4rpx;
    font-size:24rpx;
}
.userCar .footer{
    margin-top:16rpx;
    font-size:24rpx;
    color:#929292;
}
.userCar image.left{
    width:100rpx;
    margin-top:20rpx;
}
.userCar view.left{
    margin-left:40rpx;
}
.userCar view.left .top{
    height:50rpx;
    margin-top:26rpx;
    font-weight: 600;
}
.userCar view.left .footer{
    margin:0;
    font-weight: 600;
}

.function{
    padding:10rpx 20rpx;
    height:500rpx;
}
.function .fun{
    margin-top:50rpx;
    text-align: center;
    width:25%;
    height:180rpx;
    float:left;
    font-size:28rpx;
}
.function .fun image{
    width:120rpx;
    margin-bottom: 12rpx;
}

.manual image{
    width:100%;
}