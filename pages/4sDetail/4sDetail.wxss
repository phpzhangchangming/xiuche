page{
   background: #F5F5F7;
}
#head{
    width: 100%;
    height:114rpx;
    background: #F95A42;
    position:fixed;
    top:0;
    z-index: 2;
}
#head .storefront{
    font-size: 34rpx;
    font-weight: 600;
    color:#333333;
    width:90%;
    height: 60rpx;
    line-height: 60rpx;
    left:3%;
    background: #fff;
    position:fixed;
    top:40rpx;
    z-index: 2;
    padding:34rpx 2%;
    border-radius:10rpx;
}
#head .storefront image{
    width:120rpx;
    height:60rpx;
    float:left;
    margin-right:20rpx;
}
#body{
    margin-top:190rpx;
    padding-bottom: 10rpx;
}
.sa , .plant , .comment{
    background: #fff;
    border-radius: 10rpx;
    margin-bottom:20rpx;
}
.sa .top, .plant .top, .comment .top{
    font-size: 34rpx;
    font-weight: 600;
    border-bottom: 2rpx solid #E4E4E4;
    padding:28rpx 0 28rpx 50rpx;
    margin-bottom:20rpx;
}
.lists{
    padding:20rpx;
    height: 194rpx;
}
.saPN{
    position: relative;
    top:210rpx;
    z-index: 1;
}
.sa .prev,.sa .next{
    width:22rpx;
    height:22rpx;
    border-left:4rpx solid #333;
    border-bottom:4rpx solid #333;
}
.sa .prev{
    transform:rotate(45deg);
    float: left;
    margin-left:30rpx;
}
.sa .next{
    transform:rotate(-135deg);
    float: right;
    margin-right:30rpx;
}
.swiper-item{
    text-align: center;
}
.swiper-item image{
    width:120rpx;
    height:120rpx;
}
.content{
    font-size: 28rpx;
    font-weight: 600;
    color: #A2A2A2;
    margin-top:20rpx;
}
.content view{
    float: left;
    width:33.3%;
}
.content .grade{
    color: #F95A42;
}
.plantPN{
    position: relative;
    top:210rpx;
    z-index: 1;
}
.plant .prev,.plant .next{
    width:22rpx;
    height:22rpx;
    border-left:4rpx solid #333;
    border-bottom:4rpx solid #333;
}
.plant .prev{
    transform:rotate(45deg);
    float: left;
    margin-left:30rpx;
}
.plant .next{
    transform:rotate(-135deg);
    float: right;
    margin-right:30rpx;
}

.comment .top{
    margin-bottom:0;
}
.comment .commentList{
    border-bottom: 2rpx solid #E4E4E4;
    padding:30rpx 20rpx 28rpx;
}
.comment .commentList:last-child{
    border-bottom: 0;
}
.comment .commentList .userinfo image{
    float: left;
    width:80rpx;
    height:80rpx;
    margin-right:28rpx;
}
.phone{
    font-size:32rpx;
}
.time{
    font-size: 30rpx;
    color:#A8A8A8;
}
.carTyep{
    height:70rpx;
    line-height: 70rpx;
    clear: both;
}
.carTyep .left{
    font-size: 28rpx;
    color:#A8A8A8;
    margin-right: 28rpx;
}
.carTyep .right{
    color:#F95A42;
}
.text{
    font-size: 30rpx;
}
.imgList{
    border:1px solid #fff;
}
.imgList image{
    float: left;
    width:30%;
    margin-left: 2.4%;
    margin-top:16rpx;
    max-width: 240rpx;
    text-align: left;
    border-radius:8rpx;
}