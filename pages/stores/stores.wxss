.location{
    font-size:30rpx;
    font-weight:600;
    height:44rpx;
}
.location view{
    float:left;
}
.location .site{
    color:#6B6B6B;
}

.view{
    width:100%;
    height:186rpx;
    margin-top:20rpx;
    border-bottom:1px solid #ccc;
    color:#BCBCBC;
    font-weight: 600;
}
.view .left{
    float:left;
    width:30%;
}
.view .left image{
    width:80%;
}
.view .right{
    width:70%;
    float:right;
    font-size:30rpx;
}
.view .right .name{
    color:black;
}
.view .right .center {
    margin:30rpx 0;
    height:32rpx;
}
.view .right .center .orderNum{
    float:left;
}
.view .right .center .evaluate{
    float:right;
    color:#EC1F1F;
}
.view .specificLocation{
    font-size:26rpx;
}
.view .specificLocation image{
    width:50rpx;
    float:right;
}