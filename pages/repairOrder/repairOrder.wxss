page{
   background: #F5F5F7;
}
#head{
    width: 100%;
    height:114rpx;
    background: #F95A42;
    position:fixed;
    top:0;
    z-index: 2;
}
#head .storefront{
    font-size: 34rpx;
    font-weight: 600;
    color:#333333;
    width:90%;
    height: 60rpx;
    line-height: 60rpx;
    left:3%;
    background: #fff;
    position:fixed;
    top:40rpx;
    z-index: 2;
    padding:34rpx 2%;
    border-radius:10rpx;
    text-align: center;
}
#body{
    margin-top:190rpx;
    padding-bottom: 10rpx;
}
.box{
    border-radius:10rpx;
    background: #fff;
    padding: 30rpx 20rpx 20rpx;
    border-bottom: 2rpx solid #F2F2F2;
    font-size: 30rpx;
    font-weight: 600;
    margin-bottom: 20rpx;
}
.box .name{
    font-size:34rpx;
    font-weight: 600;
    padding-bottom: 22rpx;
    border-bottom: 2rpx solid #F2F2F2;
}
.box .list{
    margin-top: 26rpx;
}
.box .trademark{
    color:#BCBCBC;
    clear: both;
    font-size: 28rpx;
    padding-bottom: 2rpx;
    border-bottom: 2rpx solid #F2F2F2;
}
.box .money{
    font-size: 28rpx;
}
.box .money .left view{
    margin-top:20rpx;
    color:#6B6B6B;
}
.box .money .right{
    margin-top:46rpx;
}
.box .money .right view{
    float: left;
    line-height: 40rpx;
    margin-left:16rpx;
}
.box .money .right view:last-child{
    color: #EC1F1F;
}
.box .name .no{
    float: right;
    color: #F47D30;
    font-size: 28rpx;
}
.order{
    margin-bottom: 120rpx;
}
.order .list{
    height:40rpx;
    color:#4F525B;
    font-weight: 400;
    clear: both;
    margin-top:16rpx;
}
#footer{
    position: fixed;
    bottom:0;
    width:100%;
}
#footer view{
    width:50%;
    height: 110rpx;
    line-height: 110rpx;
    float: left;
    border-radius: 0;
    border:0;
    text-align: center;
    font-size: 30rpx
}
#footer .qx{
    background: #fff;
    color: #EC1F1F
}
#footer .ok{
    background: #EC1F1F;
    color:#fff;
}